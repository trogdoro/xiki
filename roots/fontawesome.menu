- inline/
  ! txt = ""
  ! a = ""
  ! 63.times { txt << "|*#{a}\n"; a = a.next }
  ! #txt << "|-----------------\n"
  ! a = ""
  ! 30.times { txt << "|*#{a}\n"; a = a.next }
  ! a = ""
  ! 30.times { txt << "|*#{a}\n"; a = a.next }
  ! a = ""
  ! 40.times { txt << "|*#{a}\n"; a = a.next }
  ! txt


